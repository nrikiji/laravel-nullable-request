{
    "name": "nrikiji/laravel-nullable-request",
    "description": "Package for filling an empty parameter with a request parameter that has a nullable validation rule",
    "authors": [
        {
            "name": "nrikiji",
            "email": "nrikiji@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Nrikiji\\NullableRequest\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nrikiji\\NullableRequest\\NullableRequestProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
